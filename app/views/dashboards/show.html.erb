<div class = 'col-xs-6 col-sm-6 col-xs-offset-1'>
	<b>DREAMER:</b> <span class = 'email'><%= current_user.email %></span>

	<h1>Wish Lists</h1>
	<hr />

	<% current_user.wishlists.each do |wishlist| %>
		<div class = 'wishlist'>
			<p class = 'title'><%= link_to wishlist.occasion, '#' %></p>
			<p><%= wishlist.date %></p>
		</div>
		<br />
	<% end %>
</div>